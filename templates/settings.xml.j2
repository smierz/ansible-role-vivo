<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

    <profiles>
        <profile>
            <id>defaults</id>
            <properties>
                <app-name>{{ vivo_settings_appname }}</app-name>

                <vivo-dir>{{ vivo_settings_vivodir }}</vivo-dir>
                <tomcat-dir>{{ vivo_settings_tomcatdir }}</tomcat-dir>

                <default-theme>{{ vivo_settings_defaulttheme }}</default-theme>
            </properties>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>defaults</activeProfile>
    </activeProfiles>
</settings>
